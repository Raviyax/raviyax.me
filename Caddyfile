okurahotelservices.duckdns.org, www.okurahotelservices.duckdns.org {
    root * /var/okura/html
    encode gzip
    file_server

    php_fastcgi okura:9000 {
        env PATH_INFO {path}
    }

    tls okura@gmail.com

    @hiddenFiles {
        path_regexp ^/(\\.ht|favicon\\.ico|robots\\.txt|.*\\.(css|gif|ico|jpeg|jpg|js|png))$
    }
    respond @hiddenFiles 404
}

